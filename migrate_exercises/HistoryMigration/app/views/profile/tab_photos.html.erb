<div class="contentProfile col-xl-2 col-xl-8 col-md-9">
    <div class="contentProfile__top ">
        <%=render "shared/personal_profile"%>
    </div>
    <div class="contentProfile__bot">
        <div class="row">
            <% @list_photo.each do |ele| %>
                <% if @infors[0] == current_user %>
                    <%= render "components/card_img_profile", photo: ele%>
                <% elsif ele.mode == 'Public' %>
                    <%= render "components/card_img_profile", photo: ele%>
                <%end%>
            <%end%>
        </div>
    </div>
</div>