
<div class="contentFoto col-xl-2 col-xl-8 col-md-9">
    <div class="contentFoto__heading mb-2 d-flex justify-content-center">
        <div class="btn-group contentFoto__heading-group " role="group" aria-label="Basic radio toggle button group">
            <input type="radio" class="btn-check" name="btnradio" id="btnradio1" autocomplete="off" checked>
            <label class="btn btn-outline-primary btn-Photo" for="btnradio1">
                <%= link_to 'Photo', root_path , class: 'text-decoration-none'%>
            </label>
            
            <input type="radio" class="btn-check" name="btnradio" id="btnradio2" autocomplete="off">
            <label class="btn btn-outline-primary btn-Album" for="btnradio2">
                 <%= link_to 'Album', home_album_path , class: 'text-decoration-none'%>
            </label>
        </div>
    </div>
    <div class="row contentFoto__body d-flex justify-content-center">
        <div class="cardImg col-xxl-6 col-12" >
            <div class="cardImgBorder ">
